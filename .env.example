# Gemini AI API Key (for intent parsing)
GOOGLE_GENERATIVE_AI_API_KEY=your_gemini_api_key_here

# Infura API Key (for Ethereum RPC)
NEXT_PUBLIC_INFURA_KEY=your_infura_api_key_here

# Sepolia RPC URL
SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/YOUR_INFURA_KEY

# Private Key for Contract Deployment (DO NOT COMMIT!)
PRIVATE_KEY=your_private_key_here

# Etherscan API Key (for contract verification)
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Smart Contract Addresses (will be updated after deployment)
NEXT_PUBLIC_PERMISSION_MANAGER_ADDRESS=0x0000000000000000000000000000000000000000
NEXT_PUBLIC_AUTOMATION_EXECUTOR_ADDRESS=0x0000000000000000000000000000000000000000

# Envio GraphQL Endpoint (will be updated after Envio deployment)
NEXT_PUBLIC_ENVIO_ENDPOINT=http://localhost:8080/v1/graphql

# Network Configuration
NEXT_PUBLIC_CHAIN_ID=11155111
NEXT_PUBLIC_NETWORK_NAME=sepolia

# Application Configuration
NEXT_PUBLIC_APP_NAME=Meta-Pilot AI
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Feature Flags
NEXT_PUBLIC_ENABLE_A2A_DELEGATION=true
NEXT_PUBLIC_ENABLE_GAS_OPTIMIZATION=true
NEXT_PUBLIC_ENABLE_YIELD_TRACKING=true